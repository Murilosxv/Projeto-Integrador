{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.js", 
        "use": "@vercel/node"
      },
      {
        "src": "frontend/**", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  